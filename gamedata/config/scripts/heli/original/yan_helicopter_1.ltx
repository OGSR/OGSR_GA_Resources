[logic]
active = heli_move

[heli_move]
path_move = heli_stay_1
engine_sound = false 
on_info = {+yantar_attack_start} heli_move@1

[heli_move@1]
path_move = heli_move_1
path_look = heli_look_1
on_info = {+yantar_tunnel_finish} heli_move@2

[heli_move@2]
path_move = heli_move_1
path_look = heli_look_1
on_timer = 25000 | heli_move@end

[heli_move@end]
path_move = yan_heli_move_end_1
invulnerable = true
immortal = true
engine_sound = false
mute = true
on_timer = 15000 | heli_move@silence


[heli_move@silence]
path_move = yan_heli_move_end_1
engine_sound = false
mute = true
on_timer = 35000 | %=heli_die%