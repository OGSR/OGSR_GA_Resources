;combat_ignore = true
combat_safe_altitude = -50

[heli_move@patrol]
path_move = heli_sleep
on_signal = heli_sig| heli_move@sleep
combat_safe_altitude = -50

[heli_move@sleep]
path_move = heli_sleep2
engine_sound = false
combat_safe_altitude = -50
on_timer = 1000 | heli_move@idle

