engine_sound = false
;on_info = {+esc_heli_go} heli_move@fly
combat_safe_altitude = -50
on_timer = 15000 | heli_move@fly

[heli_move@fly]
path_move = heli_fly
;combat_use_rocket = false
;on_signal = heli_radio| %+esc_heli_direct%
on_signal2 = fly_end| heli_move@patrol
;on_timer = 30000 | heli_move@fly
